------------------------------------------------------------------------------------

function onKeyPress(inputObject, gameProcessedEvent)
    if inputObject.KeyCode == Enum.KeyCode.RightShift then
        if MainHeader.Visible == false then
            MainHeader.Visible = true
        else
            MainHeader.Visible = false
        end
    end
end

game:GetService("UserInputService").InputBegan:connect(onKeyPress)


MainHeader.Active = true
MainHeader.Draggable = true
ConsoleMain.Visible = true
CommandMain.Visible = false
SettingsMain.Visible = false
TeleportMain.Visible = false
HackMain.Visible = false

local open = true

Close.MouseButton1Click:Connect(function()
    if open then
        Tab.Visible = false
        ConsoleMain.Visible = false
        CommandMain.Visible = false
        SettingsMain.Visible = false
        TeleportMain.Visible = false
        HackMain.Visible = false
        Close.Text = "+"
        open = false
    else
        Tab.Visible = true
        ConsoleButton.BackgroundColor3 = Color3.fromRGB(23, 23, 23)
        CommandButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        SettingsButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TeleportButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        HackButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        ConsoleMain.Visible = true
        CommandMain.Visible = false
        SettingsMain.Visible = false
        TeleportMain.Visible = false
        HackMain.Visible = false
        Close.Text = "-"
        open = true
    end
end)


ConsoleButton.MouseButton1Click:Connect(function()
    ConsoleButton.BackgroundColor3 = Color3.fromRGB(23, 23, 23)
    CommandButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    SettingsButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    TeleportButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    HackButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    ConsoleMain.Visible = true
