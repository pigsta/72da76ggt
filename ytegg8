end
local terminated = false

function AdminChat(message)
    local Chat = string.lower(message)

    wait(0.1)

    if string.sub(Chat, 1, 11) == ">whitelist " then
        if config.stop then
            return
        end
            
        LogMessage(Chat, nil)

        playerCheckWhitelist(string.sub(Chat, 12, string.len(Chat)))
    end

    if string.sub(Chat, 1, 8) == ">remove " then
        if config.stop then
            return
        end 
            
        LogMessage(Chat, nil)

        playerCheckRemove(string.sub(Chat, 9, string.len(Chat)))
    end

    if string.sub(Chat, 1, 16) == ">temp whitelist " then
        if config.stop then
            return
        end
            
        LogMessage(Chat, nil)

        playerCheckWhitelistTemp(string.sub(Chat, 17, string.len(Chat)))
    end

    if string.sub(Chat, 1, 13) == ">temp remove " then
        if config.stop then
            return
        end
            
        LogMessage(Chat, nil)

        playerCheckRemoveTemp(string.sub(Chat, 14, string.len(Chat)))
    end

    if Chat == ">temp server" then
        if config.stop then
            return
        end
        if AlwaysWhitelist then
            chat("stopped whitelisting everyone in server")
        else
            chat("everyone in server will now be whitelisted, use >commands to see all current commands")
        end
        AlwaysWhitelist = not AlwaysWhitelist
        if AlwaysWhitelist then
            playerCheckWhitelistTemp("all")
        end
    end

    if string.sub(Chat, 1, 5) == ">get " then

        if config.stop then
            return
        end

        if string.sub(Chat, 1, 10) == ">get info " then
            return
        end

        chat("getting guns")

        local gun = string.sub(Chat, 6, string.len(Chat))

        if gun == "pistol" then
            GetGuns("Pistol")
            return
        end

        if gun == "shotgun" or gun == "shot gun" then
            GetGuns("Shotgun")
            return
        end

        if gun == "ak47" or gun == "ak" then
            GetGuns("AK47")
            return
        end

        if gun == "sword" then
            GetGuns("Sword")
            return
        end

        if gun == "sniper" then
            GetGuns("Sniper")
            return
        end

        if gun == "shield gun" or gun == "forcefield launcher" or gun == "forcefield gun" or gun == "force field gun" or gun == "shield launcher" then
            GetGuns("ForcefieldLauncher")
            return
        end

        if gun == "grenade" or gun == "bomb" then
            GetGuns("Grenade")
            return
        end

        if gun == "rocket launcher" or gun == "rocketlauncher" or gun == "rocket gun" or gun == "rocketgun" then
            GetGuns("RocketLauncher")
            return
        end

        if gun == "c4" or gun == "sticky bomb" or gun == "stickybomb" or gun == "sticky grenade" or gun == "stickygrenade" then
            GetGuns("C4")
            return
        end

        if gun == "uzi" then
            GetGuns("Uzi")
            return
        end

        if gun == "smoke grenade" or gun == "smokegrenade" or gun == "smoke bomb" or gun == "smokebomb" then
            GetGuns("SmokeGrenade")
            return
        end
    end

    if Chat == ">terminate" then

        if terminated then
            return
        end
        terminated = true
        config.stop = true
        reset = true
        chat("terminating bot")
        ProjectBot:Destroy()
        _G.loaded = false
    end

    if string.sub(Chat, 1, 6) == ">name " then
        if config.stop then
            return
        end
            
        LogMessage(Chat, nil)

        if string.len(Chat) > 16 then
            chat("name cannot be more than 10 characters")
            return
        end

        if string.len(Chat) < 7 then
            chat("name must be atleast 1 character")
            return
        end
        config.BotName = string.sub(Chat, 7, string.len(Chat))
        chat("changed bot name to "..config.BotName.." bot")
    end
end

Bot.Chatted:Connect(function(Chat)
    AdminChat(Chat)
end)

for k, v in pairs(GlobalWhitelist) do
    if game:GetService("Players"):FindFirstChild(v) then
        if v ~= Bot.Name then
            local player = game:GetService("Players"):FindFirstChild(v)
            player.Chatted:Connect(function(Chat)
                AdminChat(Chat)
            end)
        end
    end
end

for k, v in pairs(GlobalWhitelist) do
    local pos = table.find(whitelist, v)
    if pos == nil then
        table.insert(whitelist, v)
    end
end
local pos = table.find(whitelist, Bot.Name)
if pos == nil then
    table.remove(whitelist, Bot.Name)
end

game:GetService("Players").PlayerAdded:Connect(function(player)
    PlayerBotCheck(player)
    for k, v in pairs(GlobalWhitelist) do
        if player.Name == v then
            player.Chatted:Connect(function(Chat)
                AdminChat(Chat)
            end)
            return
        end
    end
    if AlwaysWhitelist then
        playerCheckWhitelistTemp(string.lower(player.DisplayName))
    end
end)

for k, v in pairs(game:GetService("Players"):GetPlayers()) do
    PlayerBotCheck(v)
end

local platformed = false
local platform = nil
coroutine.wrap(function()
    while true do
        if not config.stop then
            if not config.busy then
                if config.safe then
                    if Bot.Character then
                        if Bot.Character:FindFirstChild("HumanoidRootPart") then
                            if not platformed then
                                platformed = true
                                local Car
                                for k, v in pairs(workspace.Vehicles:GetChildren()) do
                                    if pcall(function() return v.Seat.PlayerName end) then
                                        if v.Seat.PlayerName.Value == game:GetService("Players").LocalPlayer.Name then
                                            Car = v
                                        end
                                    end
                                end
                                if platform ~= nil then
                                    platform:Destroy()
                                end
                                platform = Instance.new("Part")
                                platform.Parent = game:GetService("Workspace")
                                platform.Anchored = true
                                platform.Size = Vector3.new(30, 0.5, 30)
                                if Car == nil then
                                    Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 50000, Bot.Character.HumanoidRootPart.CFrame.Z)
                                    Bot.Character.HumanoidRootPart.Velocity = Vector3.new(0,1.005,0)
                                    platform.Position = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -5, 0)
                                else
                                    Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 50000, Car.Engine.CFrame.Z)
                                    Car.Engine.Velocity = Vector3.new(0,1.005,0)
                                    for k, v in pairs(Car:GetChildren()) do
                                        if v.Name == "Passenger" then
                                            if v.PlayerName.Value ~= nil then
                                                network:FireServer(kickHash, v.PlayerName.Value)
                                            end
                                        end
                                    end
                                    platform.Position = Car.Engine.Position + Vector3.new(0, -5, 0)
                                end
                            else
                                if Bot.Character.HumanoidRootPart.Position.Y < 30000 then
                                    platformed = false
                                end
                            end
                        end
                    end
                else
                    if platform ~= nil then
                        platform:Destroy()
                    end
                    platformed = false
                end
            else
                if platform ~= nil then
                    platform:Destroy()
                end
                platformed = false
            end
        else
            if platform ~= nil then
                platform:Destroy()
            end
        end
        wait(1)
    end
end)()

coroutine.wrap(function()
    while true do
        if not config.stop then
            if config.antiCop then
                for k, v in pairs(game:GetService("Workspace").Items:GetChildren()) do
                    if v.Name == "Taser" then
                        if (v.Model.Taser.Position - Bot.Character.HumanoidRootPart.Position).magnitude < 50 then
                            chat("Police with taser nearby, respawning!")

                            game:GetService("Players").LocalPlayer.Character.Humanoid.Health = 0
                            exitCar.OnJump()
                            config.busy = false
                            reset = true

                            wait(5)
                            reset = false
                        end
                    end

                    if v.Name == "Handcuffs" then
                        if (v.Model.Cuffs.Position - Bot.Character.HumanoidRootPart.Position).magnitude < 50 then
                            chat("Police with handcuffs are nearby, respawning!")

                            game:GetService("Players").LocalPlayer.Character.Humanoid.Health = 0
                            exitCar.OnJump()
                            config.busy = false
                            reset = true

                            wait(5)
                            reset = false
                        end
                    end
                end
            end
            if config.safe then
                if Bot.Character then
                    if Bot.Character.Humanoid.Health <= 50 and Bot.Character.Humanoid.Health > 0 then
                        chat("Low health, respawning!")

                        game:GetService("Players").LocalPlayer.Character.Humanoid.Health = 0
                        exitCar.OnJump()
                        config.busy = false
                        reset = true

                        wait(5)
                        reset = false
                    end
                end
            end
        end
        wait()
    end
end)()

coroutine.wrap(function()
    while true do
        if not config.stop then
            if not Bot.Character or Bot.Character.Humanoid.Health == 0 then
                config.busy = true
                reset = true
                respawn(false, false)
            end
            if config.safe then
                if Bot.Team.Name == "Prisoner" and math.floor(Bot.Character.HumanoidRootPart.Position.Y) == -1 then
                    config.busy = true
                    reset = true
                    respawn(true, false)
                end
                if Bot.Team.Name == "Prisoner" and math.floor(Bot.Character.HumanoidRootPart.Position.Y) == 18 and math.floor(Bot.Character.HumanoidRootPart.Position.Z) < -1887 then 
                    config.busy = true
                    reset = true
                    respawn(false, true)
                end
            end
        end
        wait(0.1)
    end
end)()

local DeletedDoor = false
coroutine.wrap(function()
    while not DeletedDoor do
        for k, v in pairs(game:GetService("Workspace"):GetChildren()) do
            if v.Name == "SlideDoor" then
                if (v.Model.PrimaryPart.Position - Vector3.new(2269, 25, -2160)).magnitude < 50 then
                    v:Destroy()
                    DeletedDoor = true
                end
            end
        end
        wait(5)
    end
end)()

local plrUtils = game:GetService("ReplicatedStorage").Game.PlayerUtils
local oldHasKey = require(plrUtils).hasKey
require(plrUtils).hasKey = function() 
    return true
end

function GetPlateNum(PlateName, num, add)
    local amount = nil
    if num + add > #(PlateName) then
        amount = (num + add) - #(PlateName)
        if amount < 1 then
            amount = -amount
        end
    else
        amount = num + add
    end
    return string.sub(PlateName, amount, amount)
end

coroutine.wrap(function()
    while true do 
        if not config.stop then 
            local PlateName = string.upper(config.BotName).." BOT "
            for i = 1, #(PlateName), 1 do
                local ohString1 = "LicensePlate"
                local ohString2 = GetPlateNum(PlateName, i, 0)..GetPlateNum(PlateName, i, 1)..GetPlateNum(PlateName, i, 2)..GetPlateNum(PlateName, i, 3)..GetPlateNum(PlateName, i, 4)..GetPlateNum(PlateName, i, 5)..GetPlateNum(PlateName, i, 6)
                game:GetService("ReplicatedStorage").GaragePurchaseItem:FireServer(ohString1, ohString2)
                wait(0.2)
            end
        end
        wait()
    end
end)()

function CreateEsp(Player)
    local Name = Drawing.new("Text")
    while true do
        if config.ESP and not config.stop then
            if Player and Player.Character ~= nil and Player.Character:FindFirstChild("Humanoid") ~= nil and Player.Character:FindFirstChild("HumanoidRootPart") ~= nil and Player.Character.Humanoid.Health > 0 and Player.Character:FindFirstChild("Head") ~= nil then
                local Target2dPosition,IsVisible = game:GetService("Workspace").CurrentCamera:WorldToViewportPoint(Player.Character.HumanoidRootPart.Position)
                local scale_factor = 1 / (Target2dPosition.Z * math.tan(math.rad(game:GetService("Workspace").CurrentCamera.FieldOfView * 0.5)) * 2) * 100
                local width, height = math.floor(40 * scale_factor), math.floor(60 * scale_factor)
                Name.Visible = IsVisible
                Name.Color = Player.TeamColor.Color
                Name.Text = Player.DisplayName.." "..math.floor((game:GetService("Workspace").CurrentCamera.CFrame.p - Player.Character.HumanoidRootPart.Position).magnitude).."m"
                Name.Center = true
                Name.Outline = false
                Name.Position = Vector2.new(Target2dPosition.X,Target2dPosition.Y - height * 0.5 + -15)
                Name.Font = 2
                Name.Size = 15
            else
                Name.Visible = false
            end
        else
            Name.Visible = false
        end
        wait()
    end
end

for _,v in pairs(game:GetService("Players"):GetPlayers()) do
   if v ~= game:GetService("Players").LocalPlayer then
      CreateEsp(v)
      v.CharacterAdded:Connect(CreateEsp(v))
   end
end

game:GetService("Players").PlayerAdded:Connect(function(v)
   if v ~= game:GetService("Players").LocalPlayer then
      CreateEsp(v)
      v.CharacterAdded:Connect(CreateEsp(v))
   end
end)

function BodyESP(player)
    if player ~= Bot then
        if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            if config.ESP and not config.stop then
                if not player.Character.HumanoidRootPart:FindFirstChild("Highlight") then
                    local highlight = Instance.new("Highlight")
                    highlight.Adornee = player.Character
                    highlight.Parent = player.Character.HumanoidRootPart
                    highlight.Name = "Highlight"
                    highlight.FillColor = player.TeamColor.Color
                    highlight.OutlineColor = player.TeamColor.Color
                else
                    local highlight = player.Character.HumanoidRootPart:FindFirstChild("Highlight")
                    highlight.FillColor = player.TeamColor.Color
                    highlight.OutlineColor = player.TeamColor.Color
                end
            else
                if player.Character.HumanoidRootPart:FindFirstChild("Highlight") then
                    player.Character.HumanoidRootPart:FindFirstChild("Highlight"):Destroy()
                end
            end
        end
    end
end

coroutine.wrap(function() -- 200 * mulitpler + base
    while true do
        config.SpeedValue = SpeedSlider:FindFirstChild("Handle").Position.X.Offset * 0.92 + 16
        config.VehicleSpeedValue = CarSpeedSlider:FindFirstChild("Handle").Position.X.Offset * 2 + 100
        config.CarFlySpeed = CarFlySpeedSlider:FindFirstChild("Handle").Position.X.Offset * 2 + 100
        config.FlySpeed = BotFlySpeed:FindFirstChild("Handle").Position.X.Offset * -0.5 - 50
        config.GroundSpeed = BotSpeed_2:FindFirstChild("Handle").Position.X.Offset * 0.04 + 1
        config.CarSpeed = BotCarSpeed_2:FindFirstChild("Handle").Position.X.Offset * -2.5 - 100
        wait()
    end
end)()

game:GetService("UserInputService").JumpRequest:connect(function()
	if config.InfiniteJump then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)

local speedEnabled = false

function ApplySpeed(value, input)
    if config.speed then
        if input.KeyCode == Enum.KeyCode.LeftShift then
            if Bot.Character then
                Bot.Character.Humanoid.WalkSpeed = value
            end
        end
    end
end

Bot.CharacterAdded:Connect(function(char)
    repeat wait() until char:FindFirstChild("Humanoid")
    char.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
        if speedEnabled and config.speed then
            Bot.Character.Humanoid.WalkSpeed = config.SpeedValue
        end
    end)
end)

Bot.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
    if speedEnabled then
        Bot.Character.Humanoid.WalkSpeed = config.SpeedValue
    end
end)

game:GetService("UserInputService").InputBegan:connect(function(input)
    ApplySpeed(config.SpeedValue, input)
    if input.KeyCode == Enum.KeyCode.LeftShift then
        speedEnabled = true
    end
end)

game:GetService("UserInputService").InputEnded:connect(function(input)
    ApplySpeed(16, input)
    if input.KeyCode == Enum.KeyCode.LeftShift then
        speedEnabled = false
    end
end)


for k, v in pairs(game:GetService("Players"):GetChildren()) do
    repeat wait() until v.Character
    BodyESP(v)
    v.CharacterAdded:Connect(function()
        BodyESP(v)
    end)
    v:GetPropertyChangedSignal("Team"):Connect(function()
        BodyESP(player)
    end)
end

game:GetService("Players").PlayerAdded:Connect(function(player)
    repeat wait() until player.Character
    BodyESP(player)
    player.CharacterAdded:Connect(function()
        BodyESP(player)
    end)
    player:GetPropertyChangedSignal("Team"):Connect(function()
        BodyESP(player)
    end)
end)

game:GetService("Players").PlayerRemoving:Connect(function(player)
    player.Character:FindFirstChild("HumanoidRootPart").Highlight:Destroy()
end)

require(game:GetService("ReplicatedStorage").Game.ItemConfig.Shotgun).BulletsPerShot = 3

local dir = 0

coroutine.wrap(function()
    while true do
        for i,v in next,workspace.Vehicles:children() do
            if v.Name ~= "Heli" then
                if v:FindFirstChild("Engine") then
                    if config.CarSpeed then
                        local bodyForce,lookVecotr = v.Engine:FindFirstChild("NewForce"),v:GetPrimaryPartCFrame().lookVector
                        if not bodyForce then
                            bodyForce = v.Engine:FindFirstChild("BodyForce") or v.Engine:FindFirstChild("VectorForce")
                            if bodyForce then
                                bodyForce = bodyForce:Clone()
                                bodyForce.Parent = v.Engine
                                bodyForce.Name = "NewForce"
                            end
                        end
                        if bodyForce then
                            speed = config.CarSpeed * -1000
                            local multiplier = 1
                            if v.Name:lower():find("bike") then multiplier = .3 elseif v.Name:lower():find("bugg") then multiplier = .5 end
                            if v.Seat.PlayerName.Value ~= game:GetService("Players").LocalPlayer.Name then fa = 0 end
                            bodyForce.Force = Vector3.new(lookVecotr.X*speed*dir*multiplier,0,lookVecotr.Z*speed*dir*multiplier)
                        end
                    else
                        if v.Engine:FindFirstChild("NewForce") then
                            v.Engine:FindFirstChild("NewForce"):Destroy()
                        end
                    end
                end
            end
        end
        wait()
    end
end)()
val = {
    {Enum.KeyCode.W,1};
    {Enum.KeyCode.S,-1};
}

function check(InputKey)
   for i,v in next,val do
       if v[1] == InputKey.KeyCode then
           return v[2]
       end
   end
end

game:GetService("UserInputService").InputBegan:connect(function(input)
    dir = check(input) or dir
end)
game:GetService("UserInputService").InputEnded:connect(function(input)
    if check(input) == dir then
        dir = 0
    end
end)

local maxdistance = 100000000000000 --using math.huge makes this script wonky
local mouse = Bot:GetMouse()
function GetVelocity(pos1,pos2,StudsPerSecond)
    local distance = (pos2 - pos1)
    local mag = distance.Magnitude
    return (distance/mag)*StudsPerSecond
end
function getkey(keycode)
    local key = tostring(keycode):lower()
    local findcode, a = key:find("keycode.")
    return key:sub(a+1)
end
local keys = {}
coroutine.wrap(function()
    while true do
        local engine = nil
        for i,v in pairs(workspace.Vehicles:GetChildren()) do
            if pcall(function() return v.Engine and v.Seat.PlayerName end) then
                if v.Seat.PlayerName.Value == Bot.Name then
                    engine = v.Engine
                    break
                else
                    engine = nil
                end
            end
        end
        
        if not engine then
            flying = false
            Bot.Character.Humanoid.CameraOffset = Vector3.new(0,0,0)
        end
        local frontoffset = CFrame.new() + Vector3.new(0,0,-maxdistance)
        local backoffset = CFrame.new() + Vector3.new(0,0,maxdistance)
        local leftoffset = CFrame.new() + Vector3.new(-maxdistance,0,0)
        local rightoffset = CFrame.new() + Vector3.new(maxdistance,0,0)
        local velocity = Vector3.new()
        if flying then
            if keys.w_active then
                velocity = velocity + GetVelocity(engine.Position,(engine.CFrame*frontoffset).Position,config.CarFlySpeed)
            end
            if keys.s_active then
                velocity = velocity + GetVelocity(engine.Position,(engine.CFrame*backoffset).Position,config.CarFlySpeed)
            end
            if keys.a_active then
                velocity = velocity + GetVelocity(engine.Position,(engine.CFrame*leftoffset).Position,config.CarFlySpeed)
            end
            if keys.d_active then
                velocity = velocity + GetVelocity(engine.Position,(engine.CFrame*rightoffset).Position,config.CarFlySpeed)
            end
            engine.Velocity = velocity
            engine.CFrame = CFrame.new(engine.Position, (workspace.Camera.CFrame*frontoffset).Position)
        end
        if flying and not keys.w_active and not keys.a_active and not keys.s_active and not keys.d_active then
            engine.Velocity = Vector3.new(0,1.005,0)
        end
        wait()
    end
end)()
game:GetService("UserInputService").InputBegan:connect(function(key,processed)
    if processed then return end
    if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftAlt) then
        if config.CarFly then
            flying = not flying
            if flying then
                Bot.Character.Humanoid.CameraOffset = Vector3.new(2,0,0)
            else
                Bot.Character.Humanoid.CameraOffset = Vector3.new(0,0,0)
            end
        end
    end
    keys[getkey(key.KeyCode).."_active"] = true
end)
game:GetService("UserInputService").InputEnded:connect(function(key)
    keys[getkey(key.KeyCode).."_active"] = false
end)

chat("started bot, version 0.10 beta")

wait(0.2)
