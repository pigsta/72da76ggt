local flying = false
local Bot = game:GetService("Players").LocalPlayer
local ChatRemote = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
local AlwaysWhitelist = false
local Nitrix = {}
Nitrix.Backups = {}
Nitrix.Doors = {}
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local network = getupvalue(require(ReplicatedStorage.Module.AlexChassis).SetEvent, 1)
local keysList = getupvalue(getupvalue(network.FireServer, 1), 3)
local hash = 'vk464po3' -- updated
local kickHash = 'zg4iu1qx'  -- updated
local hash1 = 'ng6nb5fu'-- updated
local changehash = 'bjchha9z'-- not updated
local teamhash = 'j2qz8k5o'-- not updated
local hit = nil
local pos = nil
for i, v in pairs(getgc(true)) do
    if type(v) == "table" then
        if rawget(v, "Event") and rawget(v, "Fireworks") then
            Nitrix.em = v.em
            Nitrix.GetVehiclePacket = v.GetVehiclePacket
            Nitrix.Fireworks = v.Fireworks
            Nitrix.Network = v.Event
        elseif rawget(v, "State") and rawget(v, "OpenFun") then
            table.insert(Nitrix.Doors, v)
        elseif rawget(v, "Ragdoll") then
            Nitrix.Backups.Ragdoll = v.Ragdoll
        end
    elseif type(v) == "function" then
        if getfenv(v).script == Bot.PlayerScripts.LocalScript then
            local con = debug.getconstants(v)
            if table.find(con, "SequenceRequireState") then
                Nitrix.OpenDoor = v
            elseif table.find(con, "Play") and table.find(con, "Source") and table.find(con, "FireServer") then
                Nitrix.PlaySound = v
            elseif table.find(con, "PlusCash") then
                Nitrix.PlusCash = v
            elseif table.find(con, "Punch") then
                Nitrix.GuiFunc = v
            end
        end
    end
end

local whitelist = {}
local TempWhitelist = {}
local GlobalWhitelist = {
    "Normalkid20015_72",
    "XxHACKSPEEDxX",
    "2004DeveloperHere"
}

function load()
    local HttpService = game:GetService("HttpService")
    if readfile and isfile then 
        if isfile("whitelist.txt") then
            whitelist = HttpService:JSONDecode(readfile("whitelist.txt"))
        end
        if isfile("config.txt") then
            config = HttpService:JSONDecode(readfile("config.txt"))
        end
    else
        print("cannot load config due to injector")
    end
end

function save()
    local json
    local HttpService = game:GetService("HttpService")
    if writefile then 
        json = HttpService:JSONEncode(whitelist)
        writefile("whitelist.txt", json)
        json = HttpService:JSONEncode(config)
        writefile("config.txt", json)
    else
        print("cannot save config due to injector")
    end
end

load()
    
