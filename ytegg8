function whitelisted(name)
    for k, v in pairs(whitelist) do
        if v == name then
            return true
        end
    end
    for k, v in pairs(TempWhitelist) do
        if v == name then
            return true
        end
    end
    return false
end

require(game:GetService("ReplicatedStorage").Game.Paraglide).IsFlying = function() --Ragdoll
	return tostring(getfenv(2).script) == "Falling"
end

--AntiAFK
VirtualUser = game:service"VirtualUser"

game:service"Players".LocalPlayer.Idled:connect(function()
	VirtualUser:CaptureController()VirtualUser:ClickButton2(Vector2.new())
    Song.SoundId = "rbxassetid://6026984224"
    Song:Play()
	Notification:TweenPosition(UDim2.new(0.342, 0,0.054, 0),"Out","Quad",1)
	Notification.Text = "Roblox Tried Kicking You | Reason: AFK | Pervented: ProjectBot"
	wait(10)
	Notification:TweenPosition(UDim2.new(1.342, 0,0.054, 0),"Out","Quad",1)
	Notification.Text = ""
end)

local mouse = game:GetService('Players').LocalPlayer:GetMouse()
local function hotkeyHandler(key)
    if not config.stop then
        if (key == 'q') then
            for _, v in next, getgc(true) do
                if (type(v) == 'table' and rawget(v, 'Nitro')) then
                    v.Nitro = math.huge --250 Default
                    wait()
                end
            end
        end
    end
end
mouse.KeyDown:connect(hotkeyHandler)

function chat(message)
    if not config.private then
        ChatRemote:FireServer("["..config.BotName.." bot] "..message, "All")
    else
        for k, v in pairs(TempWhitelist) do
            pos = table.find(whitelist, v)
            if pos == nil then
                if game:GetService("Players"):FindFirstChild(v) then
                    if v ~= Bot.Name then
                        ChatRemote:FireServer("/W "..v.." ["..config.BotName.." bot] "..message, "All")
                    end
                end
            end
        end
        for k, v in pairs(whitelist) do
            if game:GetService("Players"):FindFirstChild(v) then
                if v ~= Bot.Name then
                    ChatRemote:FireServer("/W "..v.." ["..config.BotName.." bot] "..message, "All")
                end
            end
        end
    end

    LogMessage(message, nil)
end

function ChangeTeams()

    chat("changing teams")

    local name = Bot.Team.Name
    while Bot.Team.Name == name do
        network:FireServer(changehash)
        if Bot.Team.Name == "Criminal" or Bot.Team.Name == "Prisoner" then
            network:FireServer(teamhash, 'Police')
        else
            network:FireServer(teamhash, 'Prisoner')
        end
        wait(0.5)
    end

    wait(3)

    while respawning do
        wait(0.1)
    end
    if config.GiveAK then
        GetGuns("AK47")
    end
end


local Positions = {
    Vector3.new(-1099, 500, -1378),
    Vector3.new(-1544, 500, 719),
    Vector3.new(740, 500, 1062),
    Vector3.new(-1544, 500, 721)
}

local rayDirection = Vector3.new(0, -3000, 0)
local rayDirection1 = Vector3.new(0, 3000, 0)

function GetPos(location)
    local ClosestDistance = nil
    local MinDistance = math.huge
    for k, v in pairs(Positions) do
        if (v - location).Magnitude + 5 <= MinDistance then
            local IgnorePos = false
            for k, l in pairs(game:GetService("Workspace").Vehicles:GetChildren()) do
                if l.Name == "Jeep" or l.Name == "Camaro" then
                    if (Vector3.new(l.Engine.Position.X, 500, l.Engine.Position.Z) - v).magnitude < 50 then
                        if (Vector3.new(l.Engine.Position.X, 500, l.Engine.Position.Z) - v).magnitude > 5 then
                            IgnorePos = true
                        end
                    end
                end
            end
            if not IgnorePos then
                MinDistance = (v - location).Magnitude
                ClosestDistance = v
            end
        end
    end
    for k, v in pairs(game:GetService("Workspace").Vehicles:GetChildren()) do
        if v.Name == "Jeep" or v.Name == "Camaro" then
            if v.Seat.PlayerName.Value == "" then

                local rayOrigin = v.Seat.Position + Vector3.new(0, 5, 0)
                local ray = Ray.new(rayOrigin, rayDirection1)
                local rain = game:GetService("Workspace"):FindFirstChild("Rain")
                if rain ~= nil then
                    rain = rain:FindFirstChild("RainFall")
                end
                if rain ~= nil then
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                else
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                end
                
                if hit == nil then
                    if (v.Seat.Position - location).Magnitude + 5 <= MinDistance then
                        MinDistance = (v.Seat.Position - location).Magnitude
                        ClosestDistance = v.Seat.Position
                    end
                end
            end
        end
    end
    return ClosestDistance
end

function move(pos)
    pos = Vector3.new(pos.X, 500, pos.Z)
    if reset then
        return
    end
    for i,v in pairs(workspace.Vehicles:GetChildren()) do
        if pcall(function() return v.Seat.PlayerName end) then
            if v.Seat.PlayerName.Value == Bot.Name then
                Car = v
            end
        end
    end
    Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.Position.X, 500, Bot.Character.HumanoidRootPart.Position.Z)
    while (Vector3.new(Bot.Character.HumanoidRootPart.Position.X, 500, Bot.Character.HumanoidRootPart.Position.Z) - pos).magnitude > 5 do
        local amount = (Bot.Character.HumanoidRootPart.Position - pos).magnitude
        local velocity = (Bot.Character.HumanoidRootPart.Position - pos) / amount
        Bot.Character.HumanoidRootPart.Velocity = Vector3.new(velocity.X * config.FlySpeed, 0.1, velocity.Z * config.FlySpeed)
        wait()
        if reset then
            return
        end
    end
    Bot.Character.HumanoidRootPart.CFrame = CFrame.new(pos)
    Bot.Character.HumanoidRootPart.Velocity = Vector3.new()
end

function CarMove(pos)
    pos = Vector3.new(pos.X, 500, pos.Z)
    if reset then
        return
    end
    for i,v in pairs(workspace.Vehicles:GetChildren()) do
        if pcall(function() return v.Seat.PlayerName end) then
            if v.Seat.PlayerName.Value == Bot.Name then
                Car = v
            end
        end
    end
    Car.Engine.CFrame = CFrame.new(Car.Engine.Position.X, 500, Car.Engine.Position.Z)
    while (Vector3.new(Car.Engine.Position.X, 500, Car.Engine.Position.Z) - pos).magnitude > 10 do
        local amount = (Car.Engine.Position - pos).magnitude
        local velocity = (Car.Engine.Position - pos) / amount
        Car.Engine.Velocity = Vector3.new(velocity.X * config.CarSpeed, 0.1, velocity.Z * config.CarSpeed)
        wait()
        if reset then
            return
        end
    end
    Car.Engine.Velocity = Vector3.new()
end


function groundMove(pos)
    for i,v in pairs(workspace.Vehicles:GetChildren()) do
        if pcall(function() return v.Seat.PlayerName end) then
            if v.Seat.PlayerName.Value == Bot.Name then
                Car = v
            end
        end
    end

    if car == nil then
        while (Vector3.new(Bot.Character.HumanoidRootPart.Position.X, 500, Bot.Character.HumanoidRootPart.Position.Z) - Vector3.new(pos.X, 500, pos.Z)).magnitude > 10 do
            local amount = (Bot.Character.HumanoidRootPart.Position - pos).magnitude / config.GroundSpeed
            local MoveDist = (Bot.Character.HumanoidRootPart.Position - pos) / amount
            for i = 1, amount, 1 do
                Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.Position - MoveDist)
                Bot.Character.HumanoidRootPart.Velocity = Vector3.new(0,1.005,0)
                wait(0.05)
            end
            wait()
        end
        Bot.Character.HumanoidRootPart.CFrame = CFrame.new(pos)
    else
        while (Vector3.new(Car.Engine.Position.X, 500, Car.Engine.Position.Z) - Vector3.new(pos.X, 500, pos.Z)).magnitude > 10 do
            local amount = (Car.Engine.Position - pos).magnitude / config.GroundSpeed
            local MoveDist = (Car.Engine.Position - pos) / amount
            for i = 1, amount, 1 do
                Car.Engine.CFrame = CFrame.new(Car.Engine.Position - MoveDist)
                Car.Engine.Velocity = Vector3.new(0,1.005,0)
                wait(0.05)
            end
            wait()
        end
        Car.Engine.CFrame = CFrame.new(pos)
    end
end

local GunStore = false

local respawning = false

function respawn(arrested, arrested1)

    if respawning then
        return
    end

    respawning = true

    while not Bot.Character do
        wait(0.1)
    end

    while Bot.Character.Humanoid.Health == 0 do
        wait(0.1)
    end

    wait(1)

    print("respawning")

    if config.safe then
        if (Vector3.new(2009, 20, -2538) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(2102, 20, -2556))
            groundMove(Vector3.new(2214, 20, -2459))
            groundMove(Vector3.new(2282, 20, -2065))
        end

        if (Vector3.new(2141, 20, -2527) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(2211, 20, -2459))
            groundMove(Vector3.new(2282, 20, -2065))
        end

        if (Vector3.new(-234, 20, 1575) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(-242, 20, 1612))
        end

        if (Vector3.new(-246, 20, 1579) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(-242, 20, 1612))
        end

        if (Vector3.new(2213, 20, -2658) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(2176, 20, -2690))
            groundMove(Vector3.new(2069, 20, -3174))
        end

        if (Vector3.new(2135, 20, -2651) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(2176, 20, -2690))
            groundMove(Vector3.new(2069, 20, -3174))
        end

        if (Vector3.new(2103, 20, -2606) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(2211, 20, -2459))
            groundMove(Vector3.new(2282, 20, -2065))
        end

        if (Vector3.new(2073, 20, -2551) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(2211, 20, -2459))
            groundMove(Vector3.new(2282, 20, -2065))
        end

        if (Vector3.new(733, 39, 1113) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(722, 39, 1077))
        end

        if (Vector3.new(733, 39, 1113) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(722, 39, 1077))
        end

        if (Vector3.new(227, 63, 1132) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(215, 63, 1116))
            groundMove(Vector3.new(175, 63, 1113))
        end

        if (Vector3.new(-1164, 19, -1593) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(-1193, 19, -1597))
            groundMove(Vector3.new(-1192, 19, -1617))
        end

        if (Vector3.new(-1402, 20, -1759) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(-1363, 20, -1759))
        end

        if (Vector3.new(-1375, 20, -1757) - Bot.Character.HumanoidRootPart.Position).magnitude < 5 then
            groundMove(Vector3.new(-1363, 20, -1759))
        end

        if arrested then
            local minDist = math.huge
            local doorPos = nil
            for k, v in pairs(game:GetService("Workspace"):GetChildren()) do
                if v.Name == "Cell" then
                    if (v.Door.Closed.Position - Bot.Character.HumanoidRootPart.Position).magnitude < minDist then
                        doorPos = v.Door.Closed.Position
                        minDist = (v.Door.Closed.Position - Bot.Character.HumanoidRootPart.Position).magnitude
                    end
                end
            end
            if Bot.Character.HumanoidRootPart.Position.X < -1390 and math.floor(Bot.Character.HumanoidRootPart.Position.Y) < 5 then
                groundMove(Vector3.new(Bot.Character.HumanoidRootPart.Position.X, 0, doorPos.Z))
            end
            if Bot.Character.HumanoidRootPart.Position.Z < -1870 and math.floor(Bot.Character.HumanoidRootPart.Position.Y) < 5 then
                groundMove(Vector3.new(-1490, 0, -1886))
                groundMove(Vector3.new(-1482, 0, -1864))
                groundMove(Vector3.new(-1451, 20, -1864))
                groundMove(Vector3.new(-1396, 20, -1864))
                groundMove(Vector3.new(-1400, 20, -1758))
                groundMove(Vector3.new(-1360, 20, -1759))
            end
            if Bot.Character.HumanoidRootPart.Position.Z > -1815 and math.floor(Bot.Character.HumanoidRootPart.Position.Y) < 5 then
                groundMove(Vector3.new(-1393, 0, -1886))
                groundMove(Vector3.new(-1488, 0, -1886))
                groundMove(Vector3.new(-1451, 20, -1864))
                groundMove(Vector3.new(-1396, 20, -1864))
                groundMove(Vector3.new(-1400, 20, -1758))
                groundMove(Vector3.new(-1360, 20, -1759))
            end
            if Bot.Character.HumanoidRootPart.Position.X > -1390 and math.floor(Bot.Character.HumanoidRootPart.Position.Y) < 5 then
                groundMove(Vector3.new(doorPos.X, 0, Bot.Character.HumanoidRootPart.Position.Z))
                groundMove(Vector3.new(-1384, 0, -1822))
                groundMove(Vector3.new(-1490, 0, -1822))
                groundMove(Vector3.new(-1482, 0, -1864))
                groundMove(Vector3.new(-1451, 20, -1864))
                groundMove(Vector3.new(-1396, 20, -1864))
                groundMove(Vector3.new(-1400, 20, -1758))
                groundMove(Vector3.new(-1360, 20, -1759))
            end
        end
        if arrested1 then
            local minDist = math.huge
            local doorPos = nil
            for k, v in pairs(game:GetService("Workspace"):GetChildren()) do
                if v.Name == "Cell" then
                    if (v.Door.Closed.Position - Bot.Character.HumanoidRootPart.Position).magnitude < minDist then
                        doorPos = v.Door.Closed.Position
                        minDist = (v.Door.Closed.Position - Bot.Character.HumanoidRootPart.Position).magnitude
                    end
                end
            end
            groundMove(Vector3.new(Bot.Character.HumanoidRootPart.Position.X, 20, doorPos.Z + (Bot.Character.HumanoidRootPart.Position.Z - doorPos.Z)))
            groundMove(Vector3.new(-1396, 20, -1864))
            groundMove(Vector3.new(-1400, 20, -1758))
            groundMove(Vector3.new(-1360, 20, -1759))
        end
    end

    wait(1)

    if config.GiveAK then
        GetGuns("AK47")
    end
    
    config.busy = false
    reset = false
    respawning = false

    local plrUtils = game:GetService("ReplicatedStorage").Game.PlayerUtils
    local oldHasKey = require(plrUtils).hasKey
    require(plrUtils).hasKey = function() 
        return true
    end
end

local Car
local exitCar = require(game:GetService("ReplicatedStorage").Game.CharacterUtil)

function lift(location, player, LocationName, teleporting)

    if config.busy then
        chat("cant give you an uber right now")
        return
    end

    if not player.Character then
        chat("cannot find player character")
        return
    end

    if not player.Character:FindFirstChild("HumanoidRootPart") then
        chat("cannot find player location")
        return
    end

    if player == bot then
        chat("cannot uber to bot")
        return
    end

    if not teleport then
        if LocationName == "casino security" or LocationName == "casino vault" then
            chat("teleporting is disabled")
            return
        end
    end

    local rayOrigin = player.Character.HumanoidRootPart.Position + Vector3.new(0, 5, 0)
    local ray = Ray.new(rayOrigin, rayDirection1)
    local rain = game:GetService("Workspace"):FindFirstChild("Rain")
    if rain ~= nil then
        rain = rain:FindFirstChild("RainFall")
    end
    if rain ~= nil then
        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
    else
        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
    end

    if hit ~= nil then
        chat("cannot uber while under a roof")
        return
    end

    Car = nil
    config.busy = true

    for i,v in pairs(workspace.Vehicles:GetChildren()) do
        if pcall(function() return v.Seat.PlayerName end) then
            if v.Seat.PlayerName.Value == Bot.Name then
                Car = v
            end
        end
    end

    if Car == nil then
        chat("getting car")
        rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
        local ray = Ray.new(rayOrigin, rayDirection)
        rain = game:GetService("Workspace"):FindFirstChild("Rain")
        if rain ~= nil then
            rain = rain:FindFirstChild("RainFall")
        end
        if rain ~= nil then
            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
        else
            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
        end
        if hit ~= nil then
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, pos.Y, Bot.Character.HumanoidRootPart.CFrame.Z)
            wait(0.5)
        else
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
            local rayDirection2 = Vector3.new(0, -1, 0)
            while hit == nil do
                rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                ray = Ray.new(rayOrigin, rayDirection2)
                rain = game:GetService("Workspace"):FindFirstChild("Rain")
                if rain ~= nil then
                    rain = rain:FindFirstChild("RainFall")
                end
                if rain ~= nil then
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                else
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                end
                wait()
            end
        end

       Bot.Character.HumanoidRootPart.Velocity = Vector3.new()

        local args = {
        [1] = "Chassis",
        [2] = "Jeep"
        }
        game:GetService("ReplicatedStorage").GarageSpawnVehicle:FireServer(unpack(args))
    end

    wait(2)

    for i,v in pairs(workspace.Vehicles:GetChildren()) do
        if pcall(function() return v.Seat.PlayerName end) then
            if v.Seat.PlayerName.Value == Bot.Name then
                Car = v
            end
        end
    end

    while Car == nil do
        Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 500, Bot.Character.HumanoidRootPart.CFrame.Z)
        wait(1)

        move(GetPos(Bot.Character.HumanoidRootPart.Position))

        rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
        ray = Ray.new(rayOrigin, rayDirection)
        rain = game:GetService("Workspace"):FindFirstChild("Rain")
        if rain ~= nil then
            rain = rain:FindFirstChild("RainFall")
        end
        if rain ~= nil then
            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
        else
            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
        end


        if hit ~= nil then
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, pos.Y, Bot.Character.HumanoidRootPart.CFrame.Z)
            wait(0.5)
        else
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
            local rayDirection2 = Vector3.new(0, -1, 0)
            while hit == nil do
                rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                ray = Ray.new(rayOrigin, rayDirection2)
                rain = game:GetService("Workspace"):FindFirstChild("Rain")
                if rain ~= nil then
                    rain = rain:FindFirstChild("RainFall")
                end
                if rain ~= nil then
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                else
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                end
                wait()
            end
        end

        Bot.Character.HumanoidRootPart.Velocity = Vector3.new()

        local UI = require(game:GetService("ReplicatedStorage").Module:WaitForChild("UI"));
        for i,v in pairs (UI.CircleAction.Specs) do
            if v.Name == "Hijack" then
                v.Duration = 0
                v.Timed = true
            end
        end

        keypress("0x45")
        wait(0.1)
        keyrelease("0x45")

        wait(0.5)
        
        for i,v in pairs(workspace.Vehicles:GetChildren()) do
            if pcall(function() return v.Seat.PlayerName end) then
                if v.Seat.PlayerName.Value == Bot.Name then
                    Car = v
                end
                for k, l in pairs(v:GetChildren()) do
                    if l.Name == "Passanger" then
                        if l.PlayerName.Value == Bot.Name then
                            exitCar.OnJump()
                            wait(0.1)
                        end
                    end
                end
            end
        end

        if Car == nil then
            keypress("0x45")
            wait(0.1)
            keyrelease("0x45")
        end

        wait(0.5)

        for i,v in pairs(workspace.Vehicles:GetChildren()) do
            if pcall(function() return v.Seat.PlayerName end) then
                if v.Seat.PlayerName.Value == Bot.Name then
                    Car = v
                end
                for k, l in pairs(v:GetChildren()) do
                    if l.Name == "Passanger" then
                        if l.PlayerName.Value == Bot.Name then
                            exitCar.OnJump()
                            wait(0.1)
                        end
                    end
                end
            end
        end

        if reset then
            return
        end
    end

    if player.Character then
        if player.Character:FindFirstChild("HumanoidRootPart") then
            chat("going to "..player.DisplayName, player)

            Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 500, Car.Engine.CFrame.Z)
            wait(1)
            local pos = Vector3.new(player.Character.HumanoidRootPart.Position.X, 500, player.Character.HumanoidRootPart.Position.Z)
            while (Vector3.new(Car.Engine.Position.X, 500, Car.Engine.Position.Z) - Vector3.new(player.Character.HumanoidRootPart.Position.X + 5, 500, player.Character.HumanoidRootPart.Position.Z)).magnitude > 11 do
                if (pos - Vector3.new(player.Character.HumanoidRootPart.Position.X, 500, player.Character.HumanoidRootPart.Position.Z)).magnitude > 100 then 
                    chat("player moved, cancelling uber")
                    config.busy = false
                    return
                end

                CarMove(player.Character.HumanoidRootPart.Position + Vector3.new(5, 0, 0))
                
                wait()
            end
            Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 200, Car.Engine.CFrame.Z)
        else
            chat("cannot find player position")
            config.busy = false
            return
        end
    else
        chat("cannot find player body")
        config.busy = false
        return
    end

    local inCar = false
    local notInCar = 0
    while not inCar do
        notInCar += 0.05
        local avaliableSeat = false
        for k, v in pairs(Car:GetChildren()) do
            if v.Name == "Passenger" then
                if v.PlayerName.Value == player.Name then
                    inCar = true
                end

                if v.PlayerName.Value == nil then
                    avaliableSeat = true
                end
            end
            if v.Name == "Seat" then
                if v.PlayerName.Value == player.Name then
                    inCar = true
                end
            end
        end
        if not inCar and not avaliableSeat and config.safe then
            network:FireServer(kickHash, v.PlayerName.Value)
        end
        if (Vector3.new(Car.Engine.Position.X, 500, Car.Engine.Position.Z) - Vector3.new(player.Character.HumanoidRootPart.Position.X, 500, player.Character.HumanoidRootPart.Position.Z)).magnitude > 100 then
            chat("player moved, cancelling uber")
            config.busy = false
            return
        end
        if reset then
            return
        end

        if notInCar > 15 then
            chat("player didnt get in after 15 seconds, cancelling uber")
            config.busy = false
            return
        end
        wait(0.05)
    end

    chat("going to "..LocationName, player)

    if teleporting then
        local position = Vector3.new(0, 0, 0)
        local distance = 100
        while (game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position - position).magnitude > distance do
            if LocationName == "museum" then
                for i = 1, 50 do
                    Car.Engine:PivotTo(CFrame.new(1121, 116, 1296))
                    task.wait(0.01)
                end
                position = Vector3.new(1121, 116, 1296)
                distance = 500
                wait(0.01)
            end
            if LocationName == "power plant" then
                for i = 1, 50 do
                    Car.Engine:PivotTo(CFrame.new(132, 22, 2306))
                    task.wait(0.01)
                end 
                position = Vector3.new(132, 22, 2306)
                distance = 500
                wait(0.01)
            end
            if LocationName == "bank" then
                for i = 1, 50 do
                    Car.Engine:PivotTo(CFrame.new(37, 25, 864))
                    task.wait(0.01)
                end
                position = Vector3.new(37, 25, 864)
                wait(0.01)
            end
            if LocationName == "mansion" then
                for i = 1, 50 do
                    Car.Engine:PivotTo(CFrame.new(3197, 63, -4655))
                    task.wait(0.01)
                end
                position = Vector3.new(3197, 63, -4655)
                wait(0.01)
            end
            if LocationName == "jewelry store" then
                for i = 1, 50 do
                    Car.Engine:PivotTo(CFrame.new(131, 19, 1304))
                    task.wait(0.01)
                end
                position = Vector3.new(131, 19, 1304)
                wait(0.01)
            end
            if LocationName == "tomb" then
                for i = 1, 50 do
                    Car.Engine:PivotTo(CFrame.new(544, 27, -501))
                    task.wait(0.01)
                end 
                position = Vector3.new(544, 26, -522)
                wait(0.01)
            end
            if LocationName == "casino vault" then
                for i = 1, 50 do
                    Car.Engine:PivotTo(CFrame.new(250, -74, -4527))
                    task.wait(0.01)
                end
                position = Vector3.new(250, -74, -4527)
                wait(0.01)
            end
            if LocationName == "casino security" then
                for i = 1, 50 do
                    Car.Engine:PivotTo(CFrame.new(-99, 71, -4707))
                    task.wait(0.01)
                end
                position = Vector3.new(-99, 71, -4707)
                wait(0.01)
            end
            wait(1)
        end
        for i = 1, 50, 1 do
            Car.Engine:PivotTo(CFrame.new(Car.Engine.Position.X, 0, Car.Engine.Position.Z))
            wait(0.01)
        end
        if config.safe then 
            if LocationName == "bank" then
                if (Bot.Character.HumanoidRootPart.Position - Vector3.new(95, 19, 855)).magnitude > 5 then
                    groundMove(Vector3.new(0, 20, 862))
                end

                config.busy = false
                return
            end
            if LocationName == "tomb" then
                groundMove(Vector3.new(599, 25, -480))
                config.busy = false
                return
            end
            if LocationName == "museum" then
                if (Bot.Character.HumanoidRootPart.Position - Vector3.new(1121, 105, 1296)).magnitude < 5
                    groundMove(Vector3.new(1059, 106, 1247))
                end

                if (Bot.Character.HumanoidRootPart.Position - Vector3.new(1070, 105, 1321)).magnitude < 5 then
                    groundMove(Vector3.new(1048, 101, 1321))
                end

                config.busy = false
                return
            end
            config.busy = false
            Bot.Character.Humanoid.Health = 0
            return
        end
        return
    end

    if location == Vector3.new(0, 0, 0) then
        local position
        for k, v in pairs(game:GetService("Players"):GetChildren()) do
            if string.lower(v.DisplayName) == LocationName then
                if v.Character then
                    if v.Character:FindFirstChild("HumanoidRootPart") then
                        position = Vector3.new(v.Character.HumanoidRootPart.Position.X, 500, v.Character.HumanoidRootPart.Position.Z)
                    else
                        chat("cannot find player position")
                        return
                    end
                else
                    chat("cannot find player character")
                    return
                end
            end
        end

        if position == nil then
            chat("cannot find player")
            return
        end

        Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 500, Car.Engine.CFrame.Z)
        wait(1)
        while (Vector3.new(Car.Engine.Position.X, 500, Car.Engine.Position.Z) - position).magnitude > 10 do 
            CarMove(position)
            wait()
        end
    else

        Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 500, Car.Engine.CFrame.Z)
        wait(1)
        while (Vector3.new(Car.Engine.Position.X, 500, Car.Engine.Position.Z) - location).magnitude > 11 do 
            CarMove(location)
            wait()
        end
    end

    rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
    ray = Ray.new(rayOrigin, rayDirection)
    rain = game:GetService("Workspace"):FindFirstChild("Rain")
    if rain ~= nil then
        rain = rain:FindFirstChild("RainFall")
    end
    if rain ~= nil then
        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
    else
        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
    end

    if hit ~= nil then
        Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, pos.Y, Car.Engine.CFrame.Z)
    else
        Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 300, Car.Engine.CFrame.Z)
        local rayDirection2 = Vector3.new(0, -1, 0)
        while hit == nil do
            rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
            ray = Ray.new(rayOrigin, rayDirection2)
            rain = game:GetService("Workspace"):FindFirstChild("Rain")
            if rain ~= nil then
                rain = rain:FindFirstChild("RainFall")
            end
            if rain ~= nil then
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
            else
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
            end
            wait()
        end
    end

    Car.Engine.Velocity = Vector3.new()
    

    if LocationName == "volcano base" then
        wait(1)
        local pos = Vector3.new(2204, 25, -2479)
        Car.Engine.CFrame = CFrame.new(Car.Engine.Position.X, 25, Car.Engine.Position.Z)
        while (Vector3.new(Car.Engine.Position.X, 25, Car.Engine.Position.Z) - pos).magnitude > 10 do
            local amount = (Car.Engine.Position - pos).magnitude
            local velocity = (Car.Engine.Position - pos) / amount
            Car.Engine.Velocity = Vector3.new(velocity.X * config.CarSpeed, 0.1, velocity.Z * config.CarSpeed)
            wait()
            if reset then
                return
            end
        end
        Car.Engine.Velocity = Vector3.new()
    end

    wait(1)

    if config.safe then
        for k, v in pairs(Car:GetChildren()) do
            if v.Name == "Passenger" then
                network:FireServer(kickHash, v.PlayerName.Value)
            end
        end
    end

    if config.safe then
        if LocationName == "volcano base" then
            groundMove(Vector3.new(2275, 25, -2024))
        end
    end

    wait(0.5)

    config.busy = false
end

function TombPush()
    if config.busy then
        chat("cant push tomb button right now")
        return
    end

    Car = nil
    config.busy = true

    for i,v in pairs(workspace.Vehicles:GetChildren()) do
        if pcall(function() return v.Seat.PlayerName end) then
            if v.Seat.PlayerName.Value == Bot.Name then
                Car = v
            end
        end
    end

    if Car == nil then
        chat("getting car")
        rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
        ray = Ray.new(rayOrigin, rayDirection)
        rain = game:GetService("Workspace"):FindFirstChild("Rain")
        if rain ~= nil then
            rain = rain:FindFirstChild("RainFall")
        end
        if rain ~= nil then
            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
        else
            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
        end
        if hit ~= nil then
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, pos.Y, Bot.Character.HumanoidRootPart.CFrame.Z)
            wait(0.5)
        else
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
            local rayDirection2 = Vector3.new(0, -1, 0)
            while hit == nil do
                rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                ray = Ray.new(rayOrigin, rayDirection2)
                rain = game:GetService("Workspace"):FindFirstChild("Rain")
                if rain ~= nil then
                    rain = rain:FindFirstChild("RainFall")
                end
                if rain ~= nil then
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                else
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                end
                wait()
            end
        end

       Bot.Character.HumanoidRootPart.Velocity = Vector3.new()

        local args = {
        [1] = "Chassis",
        [2] = "Jeep"
        }
        game:GetService("ReplicatedStorage").GarageSpawnVehicle:FireServer(unpack(args))
    end

    wait(2)

    for i,v in pairs(workspace.Vehicles:GetChildren()) do
        if pcall(function() return v.Seat.PlayerName end) then
            if v.Seat.PlayerName.Value == Bot.Name then
                Car = v
            end
        end
    end

    while Car == nil do
        Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 500, Bot.Character.HumanoidRootPart.CFrame.Z)
        wait(1)

        move(GetPos(Bot.Character.HumanoidRootPart.Position))

        rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
        ray = Ray.new(rayOrigin, rayDirection)
        rain = game:GetService("Workspace"):FindFirstChild("Rain")
        if rain ~= nil then
            rain = rain:FindFirstChild("RainFall")
        end
        if rain ~= nil then
            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
        else
            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
        end


        if hit ~= nil then
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, pos.Y, Bot.Character.HumanoidRootPart.CFrame.Z)
            wait(1)
        else
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
            local rayDirection2 = Vector3.new(0, -1, 0)
            while hit == nil do
                rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                ray = Ray.new(rayOrigin, rayDirection2)
                rain = game:GetService("Workspace"):FindFirstChild("Rain")
                if rain ~= nil then
                    rain = rain:FindFirstChild("RainFall")
                end
                if rain ~= nil then
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                else
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                end
                wait()
            end
        end

        Bot.Character.HumanoidRootPart.Velocity = Vector3.new(0, -1, 0)

        local UI = require(game:GetService("ReplicatedStorage").Module:WaitForChild("UI"));
        for i,v in pairs (UI.CircleAction.Specs) do
            if v.Name == "Hijack" then
                v.Duration = 0
                v.Timed = true
            end
        end

        keypress("0x45")
        wait(0.1)
        keyrelease("0x45")

        wait(0.5)
        
        for i,v in pairs(workspace.Vehicles:GetChildren()) do
            if pcall(function() return v.Seat.PlayerName end) then
                if v.Seat.PlayerName.Value == Bot.Name then
                    Car = v
                end
                for k, l in pairs(v:GetChildren()) do
                    if l.Name == "Passanger" then
                        if l.PlayerName.Value == Bot.Name then
                            exitCar.OnJump()
                            wait(0.1)
                        end
                    end
                end
            end
        end

        if Car == nil then
            keypress("0x45")
            wait(0.1)
            keyrelease("0x45")
        end

        wait(0.5)

        for i,v in pairs(workspace.Vehicles:GetChildren()) do
            if pcall(function() return v.Seat.PlayerName end) then
                if v.Seat.PlayerName.Value == Bot.Name then
                    Car = v
                end
                for k, l in pairs(v:GetChildren()) do
                    if l.Name == "Passanger" then
                        if l.PlayerName.Value == Bot.Name then
                            exitCar.OnJump()
                            wait(0.1)
                        end
                    end
                end
            end
        end

        if reset then
            return
        end
    end

    chat("going to tomb")

    while (Bot.Character.HumanoidRootPart.Position - Vector3.new(544, 27, -522)).magnitude > 100 do
        for i = 1, 50 do
            Car.Engine:PivotTo(CFrame.new(544, 27, -522))
            task.wait(0.01)
        end 
        wait(1)
    end

    exitCar.OnJump()

    chat("pushing button")

    groundMove(Vector3.new(539, 25, -480))

    wait(2)

    groundMove(Vector3.new(599, 25, -480))
    config.busy = false
end

