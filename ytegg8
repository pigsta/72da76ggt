function GetPlayerPos(name)

    local target = nil
    for k, v in pairs(game:GetService("Players"):GetChildren()) do
        if string.lower(v.DisplayName) == name then
            target = v
        end
    end

    if target == nil then
        chat("cannot find player")
        return
    end

    if target.Character == nil then
        chat("player body not found")
        return
    end

    if not target.Character:FindFirstChild("HumanoidRootPart") then
        chat("cannot find player position")
        return
    end
    local NearestDistance = 99999
    local NearestLocation = nil

    for k, v in pairs(Locations) do
        if (v - target.Character.HumanoidRootPart.Position).magnitude < NearestDistance then
            NearestLocation = LocationNames[k]
            NearestDistance = (v - target.Character.HumanoidRootPart.Position).magnitude
        end
    end

    chat("the closest location to "..name.." is "..NearestLocation)
end

function BossKill()

    if not config.SilentAim then
        chat("silent aim not enabled, cant kill")
        return
    end

    Car = nil
    config.busy = true

    for i,v in pairs(workspace.Vehicles:GetChildren()) do
        if pcall(function() return v.Seat.PlayerName end) then
            if v.Seat.PlayerName.Value == Bot.Name then
                Car = v
            end
        end
    end
    local target = nil
    for k, v in pairs(game:GetService("Workspace").MansionRobbery:GetDescendants()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") then
            if v.Humanoid.Health > 0 then
                target = v
            end
        end
    end

    if target == nil then
        chat("couldnt find boss")
        config.busy = false
        return
    end


    if (Vector3.new(3214, 500, -4386) - Vector3.new(Bot.Character.HumanoidRootPart.Position.X, 500, Bot.Character.HumanoidRootPart.Position.Z)).magnitude > 5 then
        if (Bot.Character.HumanoidRootPart.Position - GetPos(Bot.Character.HumanoidRootPart.Position)).magnitude < (Vector3.new(3214, 500, -4386) - Bot.Character.HumanoidRootPart.Position).magnitude then
            if Car == nil then
                chat("getting car")

                local rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                local ray = Ray.new(rayOrigin, rayDirection)
                local rain = game:GetService("Workspace"):FindFirstChild("Rain")
                local ray = Ray.new(rayOrigin, rayDirection)
                if rain ~= nil then
                    rain = rain:FindFirstChild("RainFall")
                end
                if rain ~= nil then
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                else
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                end
                
                if hit ~= nil then
                    Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, pos.Y, Bot.Character.HumanoidRootPart.CFrame.Z)
                    wait(0.5)
                else
                    Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
                    local rayDirection2 = Vector3.new(0, -1, 0)
                    while hit == nil do
                        rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                        ray = Ray.new(rayOrigin, rayDirection2)
                        rain = game:GetService("Workspace"):FindFirstChild("Rain")
                        if rain ~= nil then
                            rain = rain:FindFirstChild("RainFall")
                        end
                        if rain ~= nil then
                            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                        else
                            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                        end
                        wait()
                    end
                end

                Bot.Character.HumanoidRootPart.Velocity = Vector3.new()

                local args = {
                [1] = "Chassis",
                [2] = "Jeep"
                }
                game:GetService("ReplicatedStorage").GarageSpawnVehicle:FireServer(unpack(args))
            end

            wait(2)

            for i,v in pairs(workspace.Vehicles:GetChildren()) do
                if pcall(function() return v.Seat.PlayerName end) then
                    if v.Seat.PlayerName.Value == Bot.Name then
                        Car = v
                    end
                end
            end

            while Car == nil do
                Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 500, Bot.Character.HumanoidRootPart.CFrame.Z)
                wait(1)
                move(GetPos(Bot.Character.HumanoidRootPart.Position))

                local rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                local ray = Ray.new(rayOrigin, rayDirection)
                local rain = game:GetService("Workspace"):FindFirstChild("Rain")
                if rain ~= nil then
                    rain = rain:FindFirstChild("RainFall")
                end
                if rain ~= nil then
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                else
                    hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                end
               
                if hit ~= nil then
                    Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, pos.Y, Bot.Character.HumanoidRootPart.CFrame.Z)
                    wait(0.5)
                else
                    Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
                    local rayDirection2 = Vector3.new(0, -1, 0)
                    while hit == nil do
                        rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                        ray = Ray.new(rayOrigin, rayDirection2)
                        rain = game:GetService("Workspace"):FindFirstChild("Rain")
                        if rain ~= nil then
                            rain = rain:FindFirstChild("RainFall")
                        end
                        if rain ~= nil then
                            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                        else
                            hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                        end
                        wait()
                    end
                end

                Bot.Character.HumanoidRootPart.Velocity = Vector3.new()

                local UI = require(game:GetService("ReplicatedStorage").Module:WaitForChild("UI"));
                for i,v in pairs (UI.CircleAction.Specs) do
                    if v.Name == "Hijack" then
                        v.Duration = 0
                        v.Timed = true
                    end
                end

                keypress("0x45")
                wait(0.1)
                keyrelease("0x45")

                wait(0.5)
                
                for i,v in pairs(workspace.Vehicles:GetChildren()) do
                    if pcall(function() return v.Seat.PlayerName end) then
                        if v.Seat.PlayerName.Value == Bot.Name then
                            Car = v
                        end
                        for k, l in pairs(v:GetChildren()) do
                            if l.Name == "Passanger" then
                                if l.PlayerName.Value == Bot.Name then
                                    exitCar.OnJump()
                                    wait(0.1)
                                end
                            end
                        end
                    end
                end

                if Car == nil then
                    keypress("0x45")
                    wait(0.1)
                    keyrelease("0x45")
                end

                wait(0.5)

                for i,v in pairs(workspace.Vehicles:GetChildren()) do
                    if pcall(function() return v.Seat.PlayerName end) then
                        if v.Seat.PlayerName.Value == Bot.Name then
                            Car = v
                        end
                        for k, l in pairs(v:GetChildren()) do
                            if l.Name == "Passanger" then
                                if l.PlayerName.Value == Bot.Name then
                                    exitCar.OnJump()
                                    wait(0.1)
                                end
                            end
                        end
                    end
                end

                wait(0.5)

                if reset then
                    return
                end
            end
        end

        chat("going to mansion")
        
        if Car == nil then
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 500, Bot.Character.HumanoidRootPart.CFrame.Z)
            wait(1)
            while (Vector3.new(Bot.Character.HumanoidRootPart.Position.X, 500, Bot.Character.HumanoidRootPart.Position.Z) - Vector3.new(3214, 500, -4386)).magnitude > 6 do
                move(Vector3.new(3214, 500, -4386))
                wait()
            end

            local rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
            local ray = Ray.new(rayOrigin, rayDirection)
            local rain = game:GetService("Workspace"):FindFirstChild("Rain")
            if rain ~= nil then
                rain = rain:FindFirstChild("RainFall")
            end
            if rain ~= nil then
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
            else
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
            end
            if hit ~= nil then
                Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, pos.Y, Bot.Character.HumanoidRootPart.CFrame.Z)
                wait(0.5)
            else
                Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
                local rayDirection2 = Vector3.new(0, -1, 0)
                while hit == nil do
                    rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                    ray = Ray.new(rayOrigin, rayDirection2)
                    rain = game:GetService("Workspace"):FindFirstChild("Rain")
                    if rain ~= nil then
                        rain = rain:FindFirstChild("RainFall")
                    end
                    if rain ~= nil then
                        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                    else
                        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                    end
                    wait()
                end
            end

            Bot.Character.HumanoidRootPart.Velocity = Vector3.new()

            wait(0.1)
        else
            Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 500, Car.Engine.CFrame.Z)
            wait(1)
            while (Vector3.new(Car.Engine.Position.X, 500, Car.Engine.Position.Z) - Vector3.new(3214, 500, -4386)).magnitude > 11 do
                CarMove(Vector3.new(3214, 500, -4386))
                wait()
            end

            local rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
            local ray = Ray.new(rayOrigin, rayDirection)
            local rain = game:GetService("Workspace"):FindFirstChild("Rain")
            if rain ~= nil then
                rain = rain:FindFirstChild("RainFall")
            end
            if rain ~= nil then
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
            else
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
            end

            if hit ~= nil then
                Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, pos.Y, Car.Engine.CFrame.Z)
            else
                Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 300, Car.Engine.CFrame.Z)
                local rayDirection2 = Vector3.new(0, -1, 0)
                while hit == nil do
                    rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                    ray = Ray.new(rayOrigin, rayDirection2)
                    rain = game:GetService("Workspace"):FindFirstChild("Rain")
                    if rain ~= nil then
                        rain = rain:FindFirstChild("RainFall")
                    end
                    if rain ~= nil then
                        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                    else
                        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                    end
                    wait()
                end
            end

            Car.Engine.Velocity = Vector3.new()

            wait(0.1)
            exitCar.OnJump()
        end
    end

    chat("killing ceo")

    pcall(function()
        toolname = "AK47"
        if Bot.Backpack:FindFirstChild(toolname) and Bot.Character:FindFirstChild(toolname) == nil then
            local tool = Bot.Backpack:FindFirstChild(toolname)
            Bot.Character.Humanoid:EquipTool(tool)
        end
    end)

    local args = {
        [1] = true
    }

    game:GetService("Players").LocalPlayer.Folder.AK47.InventoryEquipRemote:FireServer(unpack(args))

    wait(1)

    mouse1press()
    while target.Humanoid.Health > 0 do
        Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
        Bot.Character.HumanoidRootPart.Velocity = Vector3.new(0,1.005,0)
        wait(0.05)
    end
    mouse1release()

    wait(0.1)
    config.busy = false
end

function kill(name, player)

    if config.busy then
        chat("cant kill player right now")
        return
    end

    if not config.SilentAim then
        chat("silent aim not enabled, cant kill")
        return
    end

    Car = nil
    config.busy = true
    
    local target = nil
    for k, v in pairs(game:GetService("Players"):GetChildren()) do
        if string.lower(v.DisplayName) == name then
            target = v
        end
    end

    if name == "close" then
        local ClosestDistance = 99999
        local ClosestCop = nil
        for k, v in pairs(game:GetService("Players"):GetChildren()) do
            if v.Character then
                if v.Character:FindFirstChild("HumanoidRootPart") then
                    if v.Team.Name == "Police" then
                        if (v.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).magnitude < ClosestDistance then
                            ClosestCop = v
                            ClosestDistance = (v.Character.HumanoidRootPart.Position - Bot.Character.HumanoidRootPart.Position).magnitude
                        end
                    end
                end
            end
        end
        target = ClosestCop
    end

    if not Bot.Items:FindFirstChild("AK47") then
        GetGuns("AK47")
    end

    if target == nil then
        chat("cannot find player")
        config.busy = false
        return
    end

    if target.Team.Name ~= "Police" then
        if Bot.Team.Name == "Prisoner" or Bot.Team.Name == "Criminal" then
            ChangeTeams()
        end
    end

    if target.Team.Name == "Police" then
        if Bot.Team.Name == "Police" then
            ChangeTeams()
        end
    end

    if target.Character.Humanoid.Health < 0 then
        chat("player is dead")
        config.busy = false
        return
    end

    if not target.Character then
        chat("player has no body")
        config.busy = false
        return
    end

    if not target.Character:FindFirstChild("HumanoidRootPart") then
        chat("player postion doesnt exist")
        config.busy = false
        return
    end


    for i,v in pairs(workspace.Vehicles:GetChildren()) do
        if pcall(function() return v.Seat.PlayerName end) then
            if v.Seat.PlayerName.Value == Bot.Name then
                Car = v
            end
        end
    end

    if (Bot.Character.HumanoidRootPart.Position - GetPos(Bot.Character.HumanoidRootPart.Position)).magnitude < (target.Character.HumanoidRootPart.Position - Bot.Character.HumanoidRootPart.Position).magnitude then
        if Car == nil then
            chat("getting car")

            local rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
            local ray = Ray.new(rayOrigin, rayDirection)
            local rain = game:GetService("Workspace"):FindFirstChild("Rain")
            if rain ~= nil then
                rain = rain:FindFirstChild("RainFall")
            end
            if rain ~= nil then
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
            else
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
            end
            if hit ~= nil then
                Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, pos.Y, Bot.Character.HumanoidRootPart.CFrame.Z)
                wait(0.5)
            else
                Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
                local rayDirection2 = Vector3.new(0, -1, 0)
                while hit == nil do
                    rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                    ray = Ray.new(rayOrigin, rayDirection2)
                    rain = game:GetService("Workspace"):FindFirstChild("Rain")
                    if rain ~= nil then
                        rain = rain:FindFirstChild("RainFall")
                    end
                    if rain ~= nil then
                        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                    else
                        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                    end
                    wait()
                end
            end

            Bot.Character.HumanoidRootPart.Velocity = Vector3.new()

            local args = {
            [1] = "Chassis",
            [2] = "Jeep"
            }
            game:GetService("ReplicatedStorage").GarageSpawnVehicle:FireServer(unpack(args))
        end

        wait(2)

        for i,v in pairs(workspace.Vehicles:GetChildren()) do
            if pcall(function() return v.Seat.PlayerName end) then
                if v.Seat.PlayerName.Value == Bot.Name then
                    Car = v
                end
            end
        end

        while Car == nil do
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 500, Bot.Character.HumanoidRootPart.CFrame.Z)
            wait(1)

            move(GetPos(Bot.Character.HumanoidRootPart.Position))

            local rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
            local ray = Ray.new(rayOrigin, rayDirection)
            local rain = game:GetService("Workspace"):FindFirstChild("Rain")
            if rain ~= nil then
                rain = rain:FindFirstChild("RainFall")
            end
            if rain ~= nil then
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
            else
                hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
            end

            if hit ~= nil then
                Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, pos.Y, Bot.Character.HumanoidRootPart.CFrame.Z)
                wait(0.5)
            else
                Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
                local rayDirection2 = Vector3.new(0, -1, 0)
                while hit == nil do
                    rayOrigin = Bot.Character.HumanoidRootPart.Position + Vector3.new(0, -3, 0)
                    ray = Ray.new(rayOrigin, rayDirection2)
                    rain = game:GetService("Workspace"):FindFirstChild("Rain")
                    if rain ~= nil then
                        rain = rain:FindFirstChild("RainFall")
                    end
                    if rain ~= nil then
                        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {rain, Bot.Character:GetChildren()})
                    else
                        hit, pos = workspace:FindPartOnRayWithIgnoreList(ray, {})
                    end
                    wait()
                end
            end

            Bot.Character.HumanoidRootPart.Velocity = Vector3.new()

            local UI = require(game:GetService("ReplicatedStorage").Module:WaitForChild("UI"));
            for i,v in pairs (UI.CircleAction.Specs) do
                if v.Name == "Hijack" then
                    v.Duration = 0
                    v.Timed = true
                end
            end

            game:GetService('VirtualInputManager'):SendKeyEvent(true, "E", false, game)

            wait(0.5)
            
            for i,v in pairs(workspace.Vehicles:GetChildren()) do
                if pcall(function() return v.Seat.PlayerName end) then
                    if v.Seat.PlayerName.Value == Bot.Name then
                        Car = v
                    end
                    for k, l in pairs(v:GetChildren()) do
                        if l.Name == "Passanger" then
                            if l.PlayerName.Value == Bot.Name then
                                exitCar.OnJump()
                                wait(0.1)
                            end
                        end
                    end
                end
            end

            if Car == nil then
                game:GetService('VirtualInputManager'):SendKeyEvent(true, "E", false, game)
            end

            wait(0.5)

            for i,v in pairs(workspace.Vehicles:GetChildren()) do
            if pcall(function() return v.Seat.PlayerName end) then
                if v.Seat.PlayerName.Value == Bot.Name then
                    Car = v
                end
                for k, l in pairs(v:GetChildren()) do
                    if l.Name == "Passanger" then
                        if l.PlayerName.Value == Bot.Name then
                            exitCar.OnJump()
                            wait(0.1)
                        end
                    end
                end
            end
        end

            if reset then
                return
            end
        end
    end

    chat("going to "..target.DisplayName)

    if Car == nil then
        Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 500, Bot.Character.HumanoidRootPart.CFrame.Z)
        wait(1)
        local pos = Vector3.new(target.Character.HumanoidRootPart.Position.X, 500, target.Character.HumanoidRootPart.Position.Z)
        while (Vector3.new(Bot.Character.HumanoidRootPart.Position.X, 500, Bot.Character.HumanoidRootPart.Position.Z) - Vector3.new(target.Character.HumanoidRootPart.Position.X, 500, target.Character.HumanoidRootPart.Position.Z)).magnitude > 30 do
            if (pos - Vector3.new(target.Character.HumanoidRootPart.Position.X, 500, target.Character.HumanoidRootPart.Position.Z)).magnitude > 100 then 
                chat("player moved too far away, stopped killing")
                config.busy = false
                return
            end
            move(target.Character.HumanoidRootPart.Position)
            wait()
        end

        Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
        wait(0.1)
    else
        Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 500, Car.Engine.CFrame.Z)
        wait(1)
        local pos = Vector3.new(target.Character.HumanoidRootPart.Position.X, 500, target.Character.HumanoidRootPart.Position.Z)
        while (Vector3.new(Car.Engine.Position.X, 500, Car.Engine.Position.Z) - Vector3.new(target.Character.HumanoidRootPart.Position.X, 500, target.Character.HumanoidRootPart.Position.Z)).magnitude > 11 do
            if (pos - Vector3.new(target.Character.HumanoidRootPart.Position.X, 500, target.Character.HumanoidRootPart.Position.Z)).magnitude > 100 then 
                chat("player moved too far away, stopped killing")
                config.busy = false
                return
            end
            CarMove(target.Character.HumanoidRootPart.Position)
            wait()
        end
        Car.Engine.CFrame = CFrame.new(Car.Engine.CFrame.X, 300, Car.Engine.CFrame.Z)
        wait(0.1)
        exitCar.OnJump()
    end

    wait(0.5)

    chat("killing "..target.DisplayName)

    pcall(function()
        toolname = "AK47"
        if Bot.Backpack:FindFirstChild(toolname) and Bot.Character:FindFirstChild(toolname) == nil then
            local tool = Bot.Backpack:FindFirstChild(toolname)
            Bot.Character.Humanoid:EquipTool(tool)
        end
    end)

    local args = {
        [1] = true
    }

    game:GetService("Players").LocalPlayer.Folder.AK47.InventoryEquipRemote:FireServer(unpack(args))

    wait(1)

    mouse1press()
    while target.Character.Humanoid.Health > 0 do
        Bot.Character.HumanoidRootPart.CFrame = CFrame.new(Bot.Character.HumanoidRootPart.CFrame.X, 300, Bot.Character.HumanoidRootPart.CFrame.Z)
        Bot.Character.HumanoidRootPart.Velocity = Vector3.new(0,1.005,0)
        if (target.Character.HumanoidRootPart.Position - Bot.Character.HumanoidRootPart.Position).magnitude > 600 then
            chat("player moved too far away, stopped killing")
            config.busy = false
            mouse1release()
            return
        end
        wait(0.05)
    end
    mouse1release()

    wait(0.1)
    config.busy = false
end

config.SilentAim = true

getgenv().old = getgenv().old or require(game:GetService("ReplicatedStorage").Module.RayCast).RayIgnoreNonCollideWithIgnoreList

if config.SilentAim then
    require(game:GetService("ReplicatedStorage").Module.RayCast).RayIgnoreNonCollideWithIgnoreList = function(...)
        local nearestDistance, nearestEnemy = 999999, nil
        local nearestEnemyPlayer
        for i,v in pairs(game:GetService("Players"):GetPlayers()) do
            if v.Team ~= game:GetService("Players").LocalPlayer.Team and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                if v.Team.Name ~= "Prisoner" then
                    if v.Character.Humanoid.Health > 0 then
                        if (v.Character.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < nearestDistance then
                            nearestDistance, nearestEnemy = (v.Character.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude, v
                            nearestEnemyPlayer = true
                        end
                    end
                end
            end
        end
        for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
            if v.Name == "Drop" then
                for k, j in pairs(v:GetDescendants()) do
                    if j:FindFirstChild("Humanoid") and j:FindFirstChild("HumanoidRootPart") then
                        if j.Humanoid.Health > 0 then
                            if (j.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < nearestDistance then
                                nearestDistance, nearestEnemy = (j.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude, j
                                nearestEnemyPlayer = false
                            end
                        end
                    end
                end
            end
            if v.Name == "MansionRobbery" then
                for k, j in pairs(v:GetDescendants()) do
                    if j:FindFirstChild("Humanoid") and j:FindFirstChild("HumanoidRootPart") then
                        if j.Humanoid.Health > 0 then
                            if (j.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < nearestDistance then
                                nearestDistance, nearestEnemy = (j.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude, j
                                nearestEnemyPlayer = false
                            end
                        end
                    end
                end
            end
        end
        local arg = {old(...)}
        if (tostring(getfenv(2).script) == "BulletEmitter" or tostring(getfenv(2).script) == "Taser") and nearestEnemy then
            if nearestEnemyPlayer then
                arg[1] = nearestEnemy.Character.HumanoidRootPart
                arg[2] = nearestEnemy.Character.HumanoidRootPart.Position
            else
                arg[1] = nearestEnemy.HumanoidRootPart
                arg[2] = nearestEnemy.HumanoidRootPart.Position
            end
        end
        return unpack(arg)
    end
else
    require(game:GetService("ReplicatedStorage").Module.RayCast).RayIgnoreNonCollideWithIgnoreList = getgenv().old
end

function GetInfo(name)
    local player = nil
    for k, v in pairs(game:GetService("Players"):GetChildren()) do
        if string.lower(v.DisplayName) == name then
            player = v
            break
        end
    end

    if player == nil then
        chat("cant find player")
        return
    end
    local vehicle = nil
	for k, v in pairs(game:GetService("Workspace").Vehicles:GetChildren()) do
		if v:FindFirstChild("Seat") then
	    	if v.Seat.PlayerName.Value == player.Name then
				vehicle = v.Name
                break
			end
		end
	end

    local cash = player.leaderstats.Money.Value
    if string.len(tostring(cash)) >= 4 and string.len(tostring(cash)) < 7 then
        cash = math.floor(tonumber(cash) / 100) / 10
        cash = tostring(cash).."k"
    end
    if string.len(tostring(cash)) >= 7 then
        cash = math.floor(tonumber(cash) / 100000) / 10
        cash = tostring(cash).."m"
    end
    if vehicle == nil then
        chat("that players account is "..player.AccountAge.." days old, they have "..cash.." cash, they are on the "..player.Team.Name.." team and they are not in any vehicles")
    else
        chat("that players account is "..player.AccountAge.." days old, they have "..cash.." cash, they are on the "..player.Team.Name.." team and they are in a "..vehicle)
    end
end

function GetBankStatus()
    local BankStatus = game:GetService("Workspace").Banks:GetChildren()[1].Extra.Sign.Decal

    if BankStatus.Transparency == 0 then
        chat("bank is closed")
    else
        chat("bank is open")
    end
end

function GetJewelStatus()
    local JewelryStatus = workspace:FindFirstChild("Jewelrys"):GetChildren()[1].Extras.Sign.Decal
    local Wall = game:GetService("Workspace"):FindFirstChild("Jewelrys"):GetChildren()[1].Wall.SecurityWall
    if JewelryStatus.Transparency == 0 then
        chat("jewelry store is closed")
    else
        if Wall.Position.Y < 50 then
            chat("jewelry store is being robbed")
        else
            chat("jewelry store is open")
        end
    end
end

function GetMuseumStatus()
    local RoofPart = game:GetService("Workspace").Museum.Roof.Hole.RoofPart
    local DynamiteHandle = game:GetService("Workspace").Museum.Roof.Detonator0.Arm
    local UI = require(game:GetService("ReplicatedStorage").Module:WaitForChild("UI"));
    for i,v in pairs (UI.CircleAction.Specs) do
        if v.Name == "Place Dynamite" then
            v:Callback(v, true)
        end
    end
    wait(0.5)
    wait(0.1)
    if DynamiteHandle.Transparency == 1 then
        if RoofPart.Transparency > 0.5 then
            chat("museum is being robbed")
        else
            chat("museum is open")
        end
    else
        chat("museum is closed")
    end
end

function GetCasinoStatus()
    local codes = {}
    local open = false
    local robbed = false

    for k, v in pairs(game:GetService("Workspace").Casino.RobberyDoor.Codes:GetChildren()) do
        codes[k] = v
    end
    

    for k, v in pairs(codes) do
        local CodeKids = codes[k]:GetChildren()


        local Label = CodeKids[1]:GetChildren()[1]:GetChildren()[1]

        local MinX = -99999
        local MinZ = -99999
        local Pos = {}

        if Label.Text ~= "" then
            open = true
            break
        end
    end

    for k, v in pairs(game:GetService("Workspace").Casino.Computers:GetChildren()) do
        if v.Display.BrickColor == BrickColor.new("Lime green") then
            robbed = true
        end
    end

    if open then
        if robbed then
            chat("casino is being robbed")
        else
            chat("casino is open")
        end
    else
        chat("casino is closed")
    end
end

function GetMansionStatus()
    local Door = game:GetService("Workspace").MansionRobbery.Lobby.EntranceElevator.Floors:GetChildren()[1].DoorLeft.InnerModel.Door

    if Door.Position.Z > 3201 then
        chat("mansion elevator is open")
    else
        chat("mansion elevator is closed")
    end
end

function FindCode() 

    if not game:GetService("Workspace"):FindFirstChild("Casino") then
        chat("casino not loaded")
        return
    end

    local code = nil

    local codes = {}
    local Text = {}

    for k, v in pairs(game:GetService("Workspace").Casino.RobberyDoor.Codes:GetChildren()) do
        codes[k] = v
    end
    

    for k, v in pairs(codes) do
        local CodeKids = codes[k]:GetChildren()


        local Label = CodeKids[1]:GetChildren()[1]:GetChildren()[1]

        local MinX = -99999
        local MinZ = -99999
        local Pos = {}

        if Label.Text ~= "" then

            for k, v in pairs(CodeKids) do
                local X = v.Position.X
                local Z = v.Position.Z
                local placed = false
                local done = false
                Label = v:GetChildren()[1]:GetChildren()[1]
                
                print(v.Rotation.Y)
                if math.floor(v.Rotation.Y) == -75 then
                    for k, v in pairs(Pos) do
                        if Z > Pos[k] then
                            table.insert(Text, k, Label.Text)
                            table.insert(Pos.Y, k, Z)
                            placed = true
                            break
                        end
                    end
                    if not placed then
                        table.insert(Text, #(Text) + 1, Label.Text)
                        table.insert(Pos, #(Pos) + 1, Z)
                    end
                    done = true
                end
                if math.floor(v.Rotation.Y) == 14 or math.floor(v.Rotation.Y) == 15 then
                    for k, v in pairs(Pos) do
                        if Z > Pos[k] then
                            table.insert(Text, k, Label.Text)
                            table.insert(Pos, k, Z)
                            placed = true
                            break
                        end
                    end
                    if not placed then
                        table.insert(Text, #(Text) + 1, Label.Text)
                        table.insert(Pos, #(Pos) + 1, Z)
                    end
                    done = true
                end
                if not done then
                    table.insert(Text, #(Text) + 1, Label.Text)
                    table.insert(Pos, #(Pos) + 1, X)
                end

                if reset then
                    return
                end
            end

            code = Text[1]..Text[2]..Text[3]..Text[4]
        end
    end
    if code ~= nil then
        chat("casino code might be "..code, player)
    else
        chat("there is no casino code", player)
    end
end

local solving = false

function MuseumSolve()
    
    chat("solving museum puzzles")
    Door1 = game:GetService("Workspace").Museum.Puzzle1Door.Part
    Door2 = game:GetService("Workspace").Museum.Puzzle2Door.Part

    if solving then
        chat("already solving puzzles")
        return
    end

    solving = true

    while Door1.Position.X < 1081 or Door2.Position.X > 1166 do
        
        for k, v in pairs(game:GetService("Workspace").Museum.Puzzle1.Spinners:GetChildren()) do
            if v.Rotation.Y ~= 52 then
                fireclickdetector(v:GetChildren()[1])
            end
        end

        for k, v in pairs(game:GetService("Workspace").Museum.Puzzle2.Pieces:GetChildren()) do
            if v.name == "2" then
                if v.Rotation ~= Vector3.new(0,-38,180) then
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
            if v.name == "3" then
                if v.Rotation ~= Vector3.new(0,-38,0) then
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
            if v.name == "10" then
                if v.Rotation ~= Vector3.new(0,-38,180) then
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
            if v.name == "11" then
                if v.Rotation ~= Vector3.new(0,-38,180) then
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
            if v.name == "12" then
                if v.Rotation ~= Vector3.new(0,-38,0) then
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
            if v.name == "13" then
                if v.Rotation ~= Vector3.new(0,-38,90) then
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
            if v.name == "14" then
                if v.Rotation ~= Vector3.new(0,-38,0) then
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
            if v.name == "19" then
                if v.Rotation ~= Vector3.new(0,-38,180) then
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
            if v.name == "20" then
                if v.Rotation ~= Vector3.new(0,-38,-90) then
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
        end

        if reset then
            return
        end
        
        wait(0.3)
    end
    solving = false
end

function Bounty()
    local Playername = nil
    local PlayerBounty = nil

    for k, v in pairs(game:GetService("Workspace").MostWanted:GetChildren()) do
        if v:FindFirstChild("Background") then
            if v.Background:FindFirstChild("MostWanted") then
                Playername = string.lower(v.Background.MostWanted.Board.Preset.Stats.PlayerName.Text)
                PlayerBounty = string.lower(v.Background.MostWanted.Board.Preset.Stats.Bounty.Text)
            end
        end
    end

    if Playername ~= nil and PlayerBounty ~= nil then
        chat(Playername.." has the highest bounty with a "..PlayerBounty)
    else
        chat("no one has a bounty")
    end
end

hacking = false

function hackVaults()

    if hacking then
        chat("already opening vaults")
        return
    end

    hacking = true
    chat("opening vaults")

    HackingVault = false
    hacked = 0
    Rotate = true
    local VaultLight = workspace.Casino.HackableVaults.VaultDoorMain.InnerModel.Model.UnlockedLED


    while hacked < 4 do
        if not HackingVault then
            workspace.Casino.HackableVaults.VaultDoorMain.InnerModel.Puzzle.RequestHack:FireServer()
            HackingVault = true
            hacked += 1
        end
        if HackingVault then

            if VaultLight.BrickColor == BrickColor.new("Lime green") and Rotate then
                workspace.Casino.HackableVaults.VaultDoorMain.InnerModel.Puzzle.UpdateDirection:FireServer()
                hacked += 1
                Rotate = false
            end
            if VaultLight.BrickColor ~= BrickColor.new("Lime green") then
                Rotate = true
            end
            wait(0.01)
        end
        if reset then
            return
        end
    end

    for k, v in pairs(workspace.Casino.HackableVaults:GetChildren()) do
        if v.Name == "VaultDoorSlider" then

            HackingVault = false
            hacked = 0
            Rotate = true
            local VaultLight = v.InnerModel.Model.Light

            while hacked < 4 do
                if not HackingVault then
                    v.InnerModel.Puzzle.RequestHack:FireServer()
                    HackingVault = true
                    hacked += 1
                end
                if HackingVault then

                    if VaultLight.BrickColor == BrickColor.new("Lime green") and Rotate then
                        v.InnerModel.Puzzle.UpdateDirection:FireServer()
                        hacked += 1
                        Rotate = false
                    end
                    if VaultLight.BrickColor ~= BrickColor.new("Lime green") then
                        Rotate = true
                    end
                    wait(0.01)
                end
                if reset then
                    return
                end
            end
        end
    end

    hacking = false
end

function GetGuns(NAME)
    network:FireServer(hash, NAME)
end

function GiveGuns(NAME, player)

    config.busy = true

    exitCar.OnJump()

    wait(0.3)

    if player.Name == Bot.Name then
        chat("the bot cannot use >give, use >get instead")
        config.busy = false
        return
    end

    if not player.Character then
        chat("cannot find player body")
        config.busy = false
        return
    end

    if not player.Character:FindFirstChild("HumanoidRootPart") then
        chat("cannot find player location")
        config.busy = false
        return
    end

    if not Bot.Folder:FindFirstChild(NAME) then
        chat("bot doesnt own this gun")
        return
    end

    if not Bot.Items:FindFirstChild(NAME) then
        GetGuns(NAME)
    end

    chat("giving gun")

    pcall(function()
        toolname = NAME
        if Bot.Backpack:FindFirstChild(toolname) and Bot.Character:FindFirstChild(toolname) == nil then
            local tool = Bot.Backpack:FindFirstChild(toolname)
            Bot.Character.Humanoid:EquipTool(tool)
        end
    end)

    local args = { 
        [1] = true
    } 

    if game:GetService("Players").LocalPlayer.Folder:FindFirstChild(NAME) then
        game:GetService("Players").LocalPlayer.Folder:FindFirstChild(NAME).InventoryEquipRemote:FireServer(unpack(args))
    end

    wait(0.1)

    game:GetService('VirtualInputManager'):SendKeyEvent(true, "Backspace", false, game)

    wait(0.3)
    
    local oldCFrame = Bot.Character.HumanoidRootPart.CFrame
    local gun = nil
    local ClosestGun = nil
    local ClosestDistance = math.huge
    for k, v in pairs(game.Workspace:GetChildren()) do
        if v.Name == NAME then
            if v:FindFirstChild("BoundingBox") then
                if (v.BoundingBox.Position - Bot.Character.HumanoidRootPart.Position).magnitude < ClosestDistance then
                    ClosestDistance = (v.BoundingBox.Position - Bot.Character.HumanoidRootPart.Position).magnitude
                    ClosestGun = v
                end
            end
        end
    end
    gun.BoundingBox.CFrame = game:GetService("Players")[player].Character.HumanoidRootPart.CFrame

    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Players")[player].Character.HumanoidRootPart.CFrame
    wait(0.1)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = oldCFrame
end

function hack()

    if config.stop then
        return
    end

    if config.busy then
        chat("cannot hack computers right now")
        return
    end

    chat("hacking computers")
    
    for k, v in pairs(game:GetService("Workspace").Casino.Computers:GetChildren()) do
        while v.Display.BrickColor ~= BrickColor.new("Institutional white") and v.Display.BrickColor ~= BrickColor.new("Lime green") do       
            Bot.Character.HumanoidRootPart.CFrame = CFrame.new(v.Display.Position)
            v.CasinoComputerHack:FireServer()
            wait()
        end
        if v.Display.BrickColor == BrickColor.new("Lime green") then
            break
        end
    end
end

function playerCheckWhitelist(name)
    if name == "all" then
        for k, v in pairs(game:GetService("Players"):GetPlayers()) do
            local pos = table.find(whitelist, v.Name)
            if pos == nil then
                table.insert(whitelist, v.Name)
                local pos1 = table.find(TempWhitelist, v.Name)
                if pos1 == nil then
                end
            end
        end
        chat("whitelisted all players, use >commands to see all current commands")
        save()
        return
    end

    for k, v in pairs(game:GetService("Players"):GetPlayers()) do
        if name == string.lower(v.DisplayName) then
            local pos = table.find(whitelist, v.Name)
            if pos == nil then
                table.insert(whitelist, v.Name)
                chat("whitelisted "..name..", use >commands to see all current commands", player)
                local pos1 = table.find(TempWhitelist, v.Name)
                if pos1 == nil then
                end
                save()
            else 
                chat(name.." is already whitelisted", player)
            end
            return
        end
    end
    chat("couldnt find player in server")
end

function playerCheckRemove(name)
    if name == "all" then
        table.clear(whitelist)
        table.insert(whitelist, Bot.Name)
        for k, v in pairs(GlobalWhitelist) do
            table.insert(whitelist, v)
        end
        chat("removed all players from whitelist")
        save()
        return
    end

    if name == Bot.DisplayName then
        chat("cannot remove Bot")
        return
    end

    for k, v in pairs(GlobalWhitelist) do
        if name == v then
            chat("cannot remove that player")
            return
        end
    end

    for k, v in pairs(game:GetService("Players"):GetPlayers()) do
        if name == string.lower(v.DisplayName) then
            local pos = table.find(whitelist, v.Name)
            if pos ~= nil then
                table.remove(whitelist, pos)
                chat("removed "..name.." from whitelist")
                save()
            else 
                chat(name.." is not whitelisted")
            end
            return
        end
    end
    chat("couldnt find player in server")
end

function playerCheckWhitelistTemp(name)
    if name == "all" then
        for k, v in pairs(game:GetService("Players"):GetPlayers()) do
            local pos = table.find(TempWhitelist, v.Name)
            if pos == nil then
                pos = table.find(whitelist, v.Name)
            end
            if pos == nil then
                table.insert(TempWhitelist, v.Name)
            end
        end
        if not AlwaysWhitelist then
            chat("temperary whitelisted all players, use >commands to see all current commands")
        end
        return
    end

    for k, v in pairs(game:GetService("Players"):GetPlayers()) do
        if name == string.lower(v.DisplayName) then
            local pos = table.find(TempWhitelist, v.Name)
            if pos == nil then
                pos = table.find(whitelist, v.Name)
            end
            if pos == nil then
                table.insert(TempWhitelist, v.Name)
                if not AlwaysWhitelist then
                    chat("temperary whitelisted "..name..", use >commands to see all current commands")
                end
                save()
            else 
                if not AlwaysWhitelist then
                    chat(name.." is already temperary whitelisted or whitelisted", player)
                end
            end
            return
        end
    end
    chat("couldnt find player in server")
end

function playerCheckRemoveTemp(name)
    if name == "all" then
        table.clear(TempWhitelist)
        chat("removed all players from temperary whitelist")
        save()
        return
    end

    if name == Bot.DisplayName then
        chat("cannot remove Bot")
        return
    end

    for k, v in pairs(game:GetService("Players"):GetPlayers()) do
        if name == string.lower(v.DisplayName) then
            local pos = table.find(TempWhitelist, v.Name)
            if pos ~= nil then
                table.remove(TempWhitelist, pos)
                chat("removed "..name.." from temperary whitelist")
                save()
            else 
                chat(name.." is not temperary whitelisted")
            end
            return
        end
    end
    chat("couldnt find player in server")
end

function PlayerBotCheck(player)
    player.Chatted:Connect(function(message)
        PlayerChat(message, player)
    end)
end

function PlayerChat(message, player)
    if not whitelisted(player.Name) then
        return
    end

    wait(0.1)

    local Chat = string.lower(message)

    if Chat == ">uber locations" or Chat == ">uber places" or Chat == ">locations" then
        if config.stop then
            return
        end

        LogMessage(Chat, player.Name)

        if not config.uber then
            chat("uber is disabled")
            return
        end

        chat("uber locations: gas, airport, city crim, 1m, volcano, casino, mil base, msm cop, donut, town garage, crater city, city cop, town gun, [player], drop, trade, port, city garage, city gun")
        chat("more uber locations: casino vault, casino security, tomb, msm, bank, jewel, mansion, power")
        return
    end

    if Chat == ">uber town gun" or Chat == ">uber towngun" or Chat == ">uber town guns" then
        if config.stop then
            return
        end

        LogMessage(Chat, player.Name)

        if not config.uber then
            chat("uber is disabled")
            return
        end

        lift(Vector3.new(3, 500, -1760), player, "town gun store", false)
        return
    end

    if Chat == ">uber 1m" or Chat == ">uber 1million" or Chat == ">uber 1 million" or Chat == ">uber 1mil" or Chat == ">uber 1 mil" then
        if config.stop then
            return
        end

        LogMessage(Chat, player.Name)

        if not config.uber then
            chat("uber is disabled")
            return
        end

        lift(Vector3.new(474, 500, -1599), player, "1m store", false)
        return
    end

    if Chat == ">uber tomb" then
        if config.stop then
            return
        end

        LogMessage(Chat, player.Name)

        if not config.uber then
            chat("uber is disabled")
            return
        end

        lift(Vector3.new(600, 500, -468), player, "tomb", true)
        return
    end

    if Chat == ">uber casino vault" or Chat == ">uber casinovault" or Chat == ">uber casino vaults" or Chat == ">uber vault" or Chat == ">uber vaults" then
        if config.stop then
            return
        end

        LogMessage(Chat, player.Name)

        if not config.uber then
            chat("uber is disabled")
            return
        end

        lift(Vector3.new(-220, 500, -4575), player, "casino vault", true)
        return
    end

    if Chat == ">uber casino security" or Chat == ">uber casinosecurity" or Chat == ">uber security" then
        if config.stop then
            return
        end

        LogMessage(Chat, player.Name)

        if not config.uber then
            chat("uber is disabled")
            return
        end

        lift(Vector3.new(-220, 500, -4575), player, "casino security", true)
        return
    end


    if Chat == ">uber bank" then
        if config.stop then
            return
        end

        LogMessage(Chat, player.Name)

        if not config.uber then
            chat("uber is disabled")
            return
        end

        lift(Vector3.new(-1, 500, 865), player, "bank", true)
        return
    end

    if Chat == ">uber jewel" or Chat == ">uber jew" or Chat == ">uber jewelry" or Chat == ">uber j" or Chat == ">uber jewelry store" then
        if config.stop then
            return
        end

        LogMessage(Chat, player.Name)

        if not config.uber then
            chat("uber is disabled")
            return
        end

        lift(Vector3.new(80, 500, 1313), player, "jewelry store", true)
        return
    end

    if Chat == ">uber city crim" or Chat == ">uber citycrim" then
        if config.stop then
            return
