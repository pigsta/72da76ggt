local MessagePostion = 0
function CreateMessage(message)
    local NewMessage = Instance.new("TextLabel")

    NewMessage.Parent = Console
    NewMessage.BackgroundColor3 = Color3.fromRGB(23, 23, 23)
    NewMessage.BorderSizePixel = 0
    NewMessage.Position = UDim2.new(0, 0, MessagePostion, 0)
    NewMessage.Size = UDim2.new(0, 430, 0, 25)
    NewMessage.Font = Enum.Font.SourceSans
    NewMessage.Text = message
    NewMessage.TextColor3 = Color3.fromRGB(114, 114, 114)
    NewMessage.TextSize = 15.000
end

local CharPerLine = 55

function LogMessage(message, name)
    if message == nil then
        return
    end
    local lines = math.floor(#(message) / CharPerLine)

    for i = 0, lines, 1 do
        if i < lines then
            if name == nil then
                CreateMessage(string.sub(message, i * CharPerLine, ((i + 1) * CharPerLine) - 1))
            else
                CreateMessage(name..": "..string.sub(message, i * CharPerLine, ((i + 1) * CharPerLine) - 1))
            end
        else
            if name == nil then
                CreateMessage(string.sub(message, i * CharPerLine, #(message)))
            else
                CreateMessage(name..": "..string.sub(message, i * CharPerLine, #(message)))
            end
        end
        MessagePostion += 0.1
    end

    MessagePostion += 0.05
end

